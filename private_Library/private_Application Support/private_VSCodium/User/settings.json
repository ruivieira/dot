{
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "gitlens.advanced.telemetry.enabled": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.languageServer": "Pylance",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.dataScience.askForKernelRestart": false,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[xml]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "python.dataScience.alwaysTrustNotebooks": true,
  "python.linting.pylintCategorySeverity.error": "Warning",
  "python.linting.mypyCategorySeverity.error": "Warning",
  "python.linting.mypyEnabled": true,
  "python.formatting.provider": "black",
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.venvPath": "~/Library/Caches/pypoetry/virtualenvs",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "go.useLanguageServer": true,
  "workbench.colorTheme": "Gruvbox Dark Hard",
  "editor.minimap.enabled": false,
  "jupyter.alwaysTrustNotebooks": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.commandsToSkipShell": ["language-julia.interrupt"],
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter.notebook.ipynb",
      "filenamePattern": "*.ipynb"
    }
  ],
  "C_Cpp.updateChannel": "Insiders",
  "crystal-lang.completion": true,
  "crystal-lang.hover": true,
  "editor.inlineHints.fontFamily": "JuliaMono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 15,
  "crystal-ide.customCommandArgs": ["run", "~/scry/scry"],
  "crystal-lang.implementations": true,
  "crystal-lang.server": "~/scry/crystalline",
  "julia.enableTelemetry": false,
  "julia.executablePath": "/Applications/Julia-1.5.app/Contents/Resources/julia/bin/julia",
  "julia.lint.run": false,
  "graalvm.home": "~/.sdkman/candidates/java/21.0.0.2.r11-grl",
  "graalvm.installations": [
    "~/.sdkman/candidates/java/21.0.0.2.r11-grl"
  ],
  "terminal.integrated.env.osx": {
    "GRAALVM_HOME": "~/.sdkman/candidates/java/21.0.0.2.r11-grl"
  },
  "editor.fontLigatures": "true",
  "crystal-lang.compiler": "~/.asdf/shims/crystal",
  "workbench.startupEditor": "newUntitledFile",
  "editor.fontFamily": "JetBrains Mono, Monaco, 'Courier New', monospace",
  "redhat.telemetry.enabled": false,
  "prolog.executablePath": "/usr/local/Cellar/swi-prolog/8.2.4/bin/swipl",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.iconTheme": "material-icon-theme",
  "foam.dateSnippets.afterCompletion": "noop",
  "foam.graph.style": {
  
  },
  "foam.logging.level": "off"
}
